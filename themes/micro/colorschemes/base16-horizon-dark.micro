# Scheme by MichaÃ«l Ball (http://github.com/michael-ball/)

color-link default "#cbced0,#1c1e26"

color-link comment "#6f6f70"

color-link constant "#e58d7d"
color-link constant.specialChar "#e4a382"
color-link constant.string "#efaf8e"

color-link identifier "#e93c58"
color-link identifier.class "#efb993"

color-link preproc "#efb993"

color-link special "#24a8b4"

color-link statement "#e93c58"

color-link symbol.operator "#cbced0"
color-link symbol.tag "#cbced0"

color-link type "#efb993"

color-link underlined "#e93c58"

color-link error "bold #1c1e26,#e93c58"
