# Base16 Black Metal (Mayhem) - micro color scheme
# Scheme by metalelf0 (https://github.com/metalelf0)

color-link default "#c1c1c1,#000000"

color-link comment "#333333"

color-link constant "#aaaaaa"
color-link constant.specialChar "#444444"
color-link constant.string "#f3ecd4"

color-link identifier "#5f8787"
color-link identifier.class "#eecc6c"

color-link preproc "#eecc6c"

color-link special "#aaaaaa"

color-link statement "#5f8787"

color-link symbol.operator "#c1c1c1"
color-link symbol.tag "#c1c1c1"

color-link type "#eecc6c"

color-link underlined "#5f8787"

color-link error "bold #000000,#5f8787"
color-link todo "bold #eecc6c,#121212"
color-link hlsearch "#121212,#eecc6c"
color-link statusline "#999999,#222222"
color-link tabbar "#333333,#121212"
color-link indent-char "#333333"
color-link line-number "#333333,#121212"
color-link current-line-number "#999999,#121212"

color-link diff-added "#f3ecd4,#121212"
color-link diff-modified "#333333,#121212"
color-link diff-deleted "#5f8787,#121212"
color-link gutter-error "#5f8787,#121212"
color-link gutter-warning "#888888,#121212"

color-link cursor-line "#121212"
color-link color-column "#121212"
