color-link default "#4c505e,#d5d6db"

color-link comment "#9699a3"

color-link constant "#965027"
color-link constant.specialChar "#655259"
color-link constant.string "#33635c"

color-link identifier "#8c4351"
color-link identifier.class "#8f5e15"

color-link preproc "#8f5e15"

color-link special "#0f4b6e"

