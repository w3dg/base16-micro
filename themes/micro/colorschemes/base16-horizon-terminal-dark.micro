# Base16 Horizon Terminal Dark - micro color scheme
# Scheme by MichaÃ«l Ball (http://github.com/michael-ball/)

color-link default "#cbced0,#1c1e26"

color-link comment "#6f6f70"

color-link constant "#fab795"
color-link constant.specialChar "#f09383"
color-link constant.string "#29d398"

color-link identifier "#e95678"
color-link identifier.class "#fac29a"

color-link preproc "#fac29a"

color-link special "#59e1e3"

color-link statement "#e95678"

color-link symbol.operator "#cbced0"
